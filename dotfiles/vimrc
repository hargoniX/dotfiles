source ~/.vim/init-script.vim

execute pathogen#infect("~/.vim/bundle/code-analysis/{}")
source ~/.vim/bundle-init/code-analysis-init.vim

execute pathogen#infect("~/.vim/bundle/edit-utils/{}")
source ~/.vim/bundle-init/edit-utils-init.vim

execute pathogen#infect("~/.vim/bundle/colorschemes/{}")
source ~/.vim/bundle-init/colorschemes-init.vim

call pathogen#helptags()
